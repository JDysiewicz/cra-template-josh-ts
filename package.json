{
	"name": "cra-template-josh-ts",
	"version": "0.1.0",
	"license": "MIT",
	"author": "Joshua Dysiewicz <joshuadysiewicz@googlemail.com>",
	"engines": {
		"node": ">=10"
	},
	"main": "template.json",
	"description": "A quick start CRA template for TypeScript React projects containing setup for Prettier, ESLint, automated testing with Cypress, and a readme explaining how to use each technology.",
	"private": true,
	"dependencies": {
		"@emotion/core": "^11.0.0",
		"@emotion/react": "^11.1.4",
		"@emotion/styled": "^11.0.0",
		"@testing-library/jest-dom": "^5.11.4",
		"@testing-library/react": "^11.1.0",
		"@testing-library/user-event": "^12.1.10",
		"@types/jest": "^26.0.15",
		"@types/node": "^12.0.0",
		"@types/react": "^17.0.0",
		"@types/react-dom": "^16.9.8",
		"cypress": "^6.2.1",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-scripts": "4.0.1",
		"typescript": "^4.0.3",
		"web-vitals": "^0.2.4"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"lint": "node_modules/.bin/eslint --ext js,jsx,ts,tsx src",
		"cy:dev": "cypress open"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^4.12.0",
		"@typescript-eslint/parser": "^4.12.0",
		"eslint-config-prettier": "^7.1.0",
		"eslint-config-react": "^1.1.7",
		"eslint-config-react-app": "^6.0.0",
		"eslint-plugin-prettier": "^3.3.1",
		"prettier": "^2.2.1"
	}
}
